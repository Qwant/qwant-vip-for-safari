{
    "description": "Makes Qwant as the default search engine in Safari.",
    "version": "4.3.5",
    "manifest_version": 2,
    "name": "Qwant for Safari",
    "default_locale": "en",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon-128.png"
    },
    "icons": {
        "38": "icon-38.png",
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "run_at": "document_start",
            "matches": [
                "http://*/*",
                "https://*/*",
                "<all_urls>"
            ],
            "js": [
                "contentScript.bundle.js"
            ]
        },
        {
            "run_at": "document_start",
            "matches": [
                "https://*.qwant.com/*"
            ],
            "css": [
                "no-popin.css"
            ]
        }
    ],
    "permissions": [
        "<all_urls>",
        "nativeMessaging",
        "webNavigation"
    ],
    "web_accessible_resources": [
        "no-popin.css",
        "icon-128.png",
        "icon-38.png"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
